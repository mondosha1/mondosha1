    "node": ">=10.19.0"
  },
  "scripts": {
    "affected:build": "yarn nx affected:build --parallel",
    "affected:dep-graph": "yarn nx affected:dep-graph --parallel",
    "affected:e2e": "yarn nx affected:e2e --parallel",
    "affected:libs": "yarn nx affected:libs",
    "affected:lint": "yarn nx affected:lint --parallel",
    "affected:test": "yarn nx affected:test --parallel",
    "build": "yarn nx build",
    "dep-graph": "yarn nx dep-graph",
    "e2e": "yarn nx e2e",
    "format": "yarn lint:write && yarn format:write",
    "format:check": "yarn nx format:check",
    "format:write": "yarn nx format:write",
    "generate": "yarn nx generate",
    "lint": "yarn nx workspace-lint && yarn nx lint",
    "lint:write": "yarn lint --fix",
    "modclean": "./node_modules/modclean/bin/modclean.js",
    "nx": "node --max-old-space-size=6000 --stack-size=65500 ./node_modules/@nrwl/cli/bin/nx.js",
    "postinstall": "yarn modclean -r --ignore=@angular/cli/commands/*.md,highlight.js/lib/languages/makefile.js,@nrwl/linter/src/utils/convert-tslint-to-eslint/example-tslint-configs.js && ./node_modules/@angular/compiler-cli/ngcc/main-ngcc.js --properties es2015 browser module main && node ./decorate-angular-cli.js",
    "precommit": "lint-staged",
    "start": "yarn nx serve",
    "test": "yarn nx test",
    "ts-node": "node --max-old-space-size=6000 --stack-size=65500 ./node_modules/ts-node/dist/bin",
    "workspace-generator": "yarn nx workspace-generator"
  },
  "lint-staged": {
    "**/(workspace|nx|tsconfig.base).json": [
      "nxpm-stack lint",
      "prettier --write workspace.json nx.json tsconfig.base.json"
    ],
    "**/*.{json,md,html,css,scss,graphql,yaml,yml}": "prettier --write",
    "**/*.{ts,tsx,js,jsx}": [
      "yarn eslint -c ./tools/eslint-config/.eslintrc.lint-staged.json --fix",
      "prettier --write"
    ]
  },
