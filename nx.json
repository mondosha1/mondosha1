{
  "affected": {
    "defaultBase": "master"
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm",
    "warnings": {
      "typescriptMismatch": false
    }
  },
  "defaultProject": "tera",
  "generators": {
    "@nrwl/angular:application": {
      "e2eTestRunner": "cypress",
      "linter": "eslint",
      "prefix": "el",
      "routing": true,
      "strict": true,
      "style": "scss",
      "tags": "scope:???,type:app,platform:desktop",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "directory": "tera",
      "lazy": true,
      "linter": "eslint",
      "prefix": "el",
      "routing": true,
      "tags": "scope:webapp,type:feature,platform:desktop",
      "unitTestRunner": "jest"
    },
    "@nrwl/react": {
      "application": {
        "babel": true,
        "linter": "eslint",
        "style": "scss"
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "linter": "eslint",
        "style": "scss"
      }
    },
    "@schematics/angular:component": {
      "changeDetection": "OnPush",
      "prefix": "el",
      "style": "scss"
    }
  },
  "implicitDependencies": {
    ".env": "*",
    ".eslintrc.json": "*",
    ".github/workflows/**": "*",
    "package.json": "*",
    "tsconfig.base.json": "*"
  },
  "npmScope": "mondosha1",
  "targetDependencies": {
    "deploy": [
      {
        "target": "docker-tag",
        "projects": "dependencies"
      }
    ],
    "publish": [
      {
        "target": "build",
        "projects": "self"
      }
    ]
  },
  "workspaceLayout": {
    "libsDir": "packages"
  }
}
